{
  "id": "http://bars-open.ru/medvtr/json-schema/base/refbook/RbService.json",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Справочник тестов ТМИС",
  "javaType": "ru.bars_open.medvtr.mq.entities.base.refbook.RbService",
  "properties": {
    "id": {
      "type": "integer",
      "description": "Идентификатор МИС (RbService.id)",
      "minimum": 0
    },
    "code": {
      "type": "string",
      "description": "Код записи справочника (RbService.code)"
    },
    "name": {
      "type": "string",
      "description": "Наименование записи справочника (RbService.name)"
    },
    "isComplex": {
      "type": "boolean",
      "description": "является комплексной"
    }
  },
  "required": [
    "id",
    "code",
    "isComplex"
  ]
}