{
  "id": "http://bars-open.ru/medvtr/json-schema/VMPTicket.json",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "VMPTicket",
  "description": "Талон ВМП",
  "type": "object",
  "javaType": "ru.bars_open.medvtr.mq.entities.VMPTicket",
  "properties": {
    "id": {
      "type": "integer",
      "description": "Идентифкатор МИС",
      "minimum": 0
    },
    "number": {
      "type": "string",
      "description": "номер талона"
    },
    "begDate": {
      "type": "string",
      "description": "дата и время начала действия талона",
      "format": "date-time"
    },
    "endDate": {
      "type": "string",
      "description": "дата и время окончания действия талона",
      "format": "date-time"
    },
    "treatment": {
      "type": "object",
      "$ref": "./refbook/treatment.json",
      "description": "Метод лечения"
    }
  },
  "required": [
    "id",
    "number"
  ]
}