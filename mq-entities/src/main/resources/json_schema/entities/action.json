{
  "id": "http://bars-open.ru/medvtr/json-schema/action.json",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Action",
  "description": "Экшен-поступления в стационаре",
  "type": "object",
  "javaType": "ru.bars_open.medvtr.mq.entities.Action",
  "properties": {
    "id": {
      "type": "integer",
      "description": "Идентификатор экшена",
      "minimum": 0
    },
    "status": {
      "enum": [
        "STARTED",
        "WAIT",
        "FINISHED"
      ],
      "description": "Статус экшена"
    },
    "begDate": {
      "description": "Дата начала работы",
      "type": "string",
      "format": "date-time"
    },
    "endDate": {
      "description": "Дата окончания работы",
      "type": "string",
      "format": "date-time"
    }
  },
  "required": [
    "id",
    "code"
  ]
}